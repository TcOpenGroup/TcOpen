    <Method Name="Restore" Id="{368de314-cdb4-09a8-28ba-fffc6fc2c685}">
      <Declaration><![CDATA[(*~
    <docu>
        <summary>
            Restore all tasks of this instance into initial state.
        </summary>
    </docu>
~*)
METHOD Restore : ITcoRestorable]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF (TurnTask <> 0) THEN
    TurnTask.Restore();
END_IF;]]></ST>
      </Implementation>
    </Method>
