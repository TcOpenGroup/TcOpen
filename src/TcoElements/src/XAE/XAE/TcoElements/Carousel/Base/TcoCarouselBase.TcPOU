  <POU Name="TcoCarouselBase" Id="{571c422a-c609-0b64-1fef-a02a776dca84}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK ABSTRACT TcoCarouselBase EXTENDS TcoCore.TcoComponent
VAR_IN_OUT
    (*~
        <docu>
            <summary>
                In position sensor.
            </summary>
        </docu>
    ~*)
    inoInPosition : BOOL;
    (*~
        <docu>
            <summary>
                In precise position sensor.
            </summary>
        </docu>
    ~*)
    inoInPrecisePosition : BOOL;
END_VAR
VAR_IN_OUT
    (*~
        <docu>
            <summary>
                Enable turn signal.
            </summary>
        </docu>
    ~*)
    {attribute addProperty Name "<#Enable turn#>"}
    inoEnableTurn : BOOL;
    (*~
        <docu>
            <summary>
                Start turn  signal.
            </summary>
        </docu>
    ~*)
    {attribute addProperty Name "<#Start turn#>"}
    inoStartTurn : BOOL;
END_VAR
VAR
    {attribute addProperty Name "<#In position sensor#>"}
    _inPosition : BOOL;
    {attribute addProperty Name "<#In precise position sensor#>"}
    _inPrecisePosition : BOOL;
    (*~
        <docu>
            <summary>
                Enable turn  signal.
            </summary>
        </docu>
    ~*)
    {attribute addProperty Name "<#Enable turn table#>"}
    _enableTurn : BOOL;
    (*~
        <docu>
            <summary>
                Start trun signal.
            </summary>
        </docu>
    ~*)
    {attribute addProperty Name "<#Start turn table#>"}
    _startTurn : BOOL;
END_VAR
VAR
    {attribute addProperty Name "<#In position signal#>"}
    _InPositionSignalInfo : TcoCore.TcoSignalInfo;
    {attribute addProperty Name "<#In precise position signal#>"}
    _InPrecisePositionSignalInfo : TcoCore.TcoSignalInfo;
    {attribute addProperty Name "<#Enable turn signal#>"}
    _EnableTurnSignalInfo : TcoCore.TcoSignalInfo;
    {attribute addProperty Name "<#Start turn signal#>"}
    _StartTurnSignalInfo : TcoCore.TcoSignalInfo;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[IF (__ISVALIDREF(inoInPosition)) THEN
    _inPosition := inoInPosition;
    _InPositionSignalInfo.GetSignalInfo(SIZEOF(inoInPosition), ADR(inoInPosition));
END_IF;

IF (__ISVALIDREF(inoInPrecisePosition)) THEN
    _inPrecisePosition := inoInPrecisePosition;
    _InPrecisePositionSignalInfo.GetSignalInfo(SIZEOF(inoInPrecisePosition), ADR(inoInPrecisePosition));
END_IF;

IF (__ISVALIDREF(inoEnableTurn)) THEN
    inoEnableTurn := _enableTurn;
    _EnableTurnSignalInfo.GetSignalInfo(SIZEOF(inoEnableTurn), ADR(inoEnableTurn));
END_IF;

IF (__ISVALIDREF(inoStartTurn)) THEN
    inoStartTurn := _startTurn;
    _StartTurnSignalInfo.GetSignalInfo(SIZEOF(inoStartTurn), ADR(inoStartTurn));
END_IF;]]></ST>
    </Implementation>
