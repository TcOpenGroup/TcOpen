    </Compile>
    <Compile Update="TcoDigitalInspector\Shadow\TcoDigitalInspectorShadowDisplayView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="TcoDigitalInspector\Online\TcoDigitalInspectorDisplayView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>

		
	
  <ItemGroup>
    <EmbeddedResource Update="Properties\strings.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>strings.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Page Update="TcoAnalogueInpector\Online\TcoAnalogueInspectorControlView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="TcoAnalogueInpector\Online\TcoAnalogueInspectorDisplayView.xaml">
