        IF NOT _done THEN
            _sut_A._RunExecutionSubstitute();
            _sut_N._RunExecutionSubstitute();
            _sut_A.SaveNumberOfStepsInSequence();
            _sut_N.SaveNumberOfStepsInSequence();
            _done := _plcCycleCounter >= 2;
        END_IF

        IF _done THEN
            ProbeDoneWhen(_done);
        END_IF
        {endregion}
