<Application
    x:Class="Sandbox.IntegrationProjects.Wpf.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Sandbox.IntegrationProjects.Wpf"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" xmlns:tcocore="clr-namespace:TcoCore.Wpf;assembly=TcOpen.Inxton.TcoCore.Wpf"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <materialDesign:BundledTheme
                    BaseTheme="Light"
                    PrimaryColor="LightBlue" 
                    SecondaryColor="Amber" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
                <tcocore:TcoResources></tcocore:TcoResources>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
