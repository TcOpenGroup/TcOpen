<UserControl
  x:Class="TcoIo.InputBase_8311D824HWDiagStructView"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:local="clr-namespace:TcoIo"
  xmlns:vortex="http://vortex.mts/xaml"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:TcoIo="clr-namespace:TcoIo;assembly=TcoIoConnector"
  xmlns:conv="clr-namespace:TcoIo.Converters"
  d:DesignHeight="450"
  d:DesignWidth="800"
  mc:Ignorable="d"
>
    <d:UserControl.DataContext>
        <local:InputBase_8311D824HWDiagViewModel />
    </d:UserControl.DataContext>
    <StackPanel>
        <Grid Grid.IsSharedSizeScope="True" DataContext="{Binding Component}">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <TextBlock
          Grid.Row="0"
          Margin="0,5,5,0"
          Text="{Binding AttributeName}"
          Visibility="{Binding Converter={conv:NonGenericTypeToVisibleConverter}}"
        />
                <vortex:RenderableContentControl
          Grid.Row="1"
          DataContext="{Binding }"
          PresentationType="HWDiag-HWDiagLimit-DisplaySlim"
        />
            </Grid>
        </Grid>
    </StackPanel>
</UserControl>
