<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Vortex.Presentation.Controls.Wpf.Style">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="GroupBox.xaml" />
        <ResourceDictionary Source="TextBox.xaml" />
        <ResourceDictionary Source="..\Colors\Colors.xaml" />
    </ResourceDictionary.MergedDictionaries>


    <Style BasedOn="{StaticResource GroupBoxDefault}" TargetType="{x:Type GroupBox}" />
    <Style BasedOn="{StaticResource TextBoxDefault}" TargetType="{x:Type TextBox}" />

</ResourceDictionary>