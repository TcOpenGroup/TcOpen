<UserControl x:Class="TcOpen.Inxton.Security.Wpf.InsufficientRightsBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TcOpen.Inxton.Security.Wpf"
             xmlns:p="clr-namespace:TcOpen.Inxton.Security.Wpf.Properties"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.DataContext>
        <local:InsufficientRightsBoxViewModel></local:InsufficientRightsBoxViewModel>
    </UserControl.DataContext>
    <Viewbox>
        <Button HorizontalAlignment="Center" 
                VerticalAlignment="Center"                
                Command="{Binding OpenLoginWindowCommand}"
                Content="{x:Static p:strings.Login}"></Button>
    </Viewbox>
</UserControl>
